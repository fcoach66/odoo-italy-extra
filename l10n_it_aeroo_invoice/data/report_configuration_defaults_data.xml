<odoo noupdate="1">
    <record model="report.configuration.default" id="conf_line_force_code">
        <field name="name">force_code</field>
        <field name="apply_to_all" eval="False"/>
        <field name="apply_to_model_id" ref="account.model_account_invoice"/>
        <field name="override_values" eval="False"/>
        <field name="value_type">text</field>
    </record>
    <record model="report.configuration.default" id="conf_line_force_title">
        <field name="name">force_title</field>
        <field name="apply_to_all" eval="False"/>
        <field name="apply_to_model_id" ref="account.model_account_invoice"/>
        <field name="override_values" eval="False"/>
        <field name="value_type">text</field>
    </record>
    <record model="report.configuration.default" id="conf_line_print_code">
        <field name="name">print_code</field>
        <field name="apply_to_all" eval="False"/>
        <field name="apply_to_model_id" ref="account.model_account_invoice"/>
        <field name="override_values" eval="False"/>
        <field name="value_type">boolean</field>
        <field name="value_boolean" eval="True"/>
    </record>
    <record model="report.configuration.default" id="report_configuration_invoice_list_price">
        <field name="name">print_list_price_and_total_discount</field>
        <field name="apply_to_all" eval="False"/>
        <field name="apply_to_model_id" ref="account.model_account_invoice"/>
        <field name="override_values" eval="False"/>
        <field name="value_type">boolean</field>
        <field name="value_boolean" eval="False"/>
    </record>
</odoo>